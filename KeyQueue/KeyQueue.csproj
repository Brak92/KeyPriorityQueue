<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>KeyPriorityQueue</PackageId>
		<Description>A Keyed PriorityQueue that allows you to Queue method execution based on different priorities with keys. So each key has its own priority. </Description>
		<PackageTags>csharp;queue;priority;priorityqueue;keyqueue;keypriorityqueue</PackageTags>
		<RepositoryUrl>https://github.com/Brak92/KeyPriorityQueue</RepositoryUrl>
	</PropertyGroup>

</Project>
